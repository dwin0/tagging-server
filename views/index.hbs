<h1>{{title}}</h1>
<p>Welcome to {{title}}</p>

<div class="form-style-5">
    <form action="/getNearest" method="get">
        <label for="longitude1">Longitude 1: </label>
        <input type="text" name="longitude1" id="longitude1"/>
        <label for="latitude1">Latitude 1: </label>
        <input type="text" name="latitude1" id="latitude1"/>
        <hr />
        <br />

        <label for="longitude2">Longitude 2: </label>
        <input type="text" name="longitude2" id="longitude2"/>
        <label for="latitude2">Latitude 2: </label>
        <input type="text" name="latitude2" id="latitude2"/>
        <hr />
        <br />

        <label for="longitude3">Longitude 3: </label>
        <input type="text" name="longitude3" id="longitude3"/>
